{
  "name": "nuxt-ripple",
  "version": "0.0.5",
  "description": "Add simple ripple effects to your elements",
  "keywords": [
    "nuxt",
    "ripple",
    "vue",
    "css"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahmedrangel/nuxt-ripple.git"
  },
  "homepage": "https://github.com/ahmedrangel/nuxt-ripple",
  "author": {
    "name": "Ahmed Rangel",
    "email": "ahmedrangel@outlook.com",
    "url": "https://ahmedrangel.com/"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run prepack && npm run test && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run --reporter=verbose",
    "test:types": "vue-tsc --noEmit",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.14.1592"
  },
  "devDependencies": {
    "@nuxt/content": "^2.13.4",
    "@nuxt/devtools": "^1.6.1",
    "@nuxt/eslint-config": "^0.7.2",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.14.1592",
    "@nuxt/test-utils": "^3.14.4",
    "@nuxt/ui": "2.19.2",
    "@types/color": "^4.2.0",
    "@types/node": "^22.10.0",
    "changelogen": "^0.5.7",
    "color": "^4.2.3",
    "eslint": "^9.14.0",
    "nuxt": "^3.14.1592",
    "vitest": "^2.1.5",
    "vue-tsc": "^2.1.10"
  },
  "packageManager": "pnpm@9.14.2",
  "changelog": {
    "repo": {
      "repo": "ahmedrangel/nuxt-ripple",
      "provider": "github",
      "domain": "github.com"
    }
  }
}